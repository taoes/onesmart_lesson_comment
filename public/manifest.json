{
  "name": "精锐评课辅助1.1",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "精锐教务",
  "browser_action": {
    "default_icon": "./img/favicon.png",
    "default_title": "精锐教育评课辅助插件",
    "default_popup": "index.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "./img/favicon.png",
    "48": "./img/favicon.png",
    "128": "./img/favicon.png"
  },
  "background": {
    "scripts": [
      "script/background/index.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://guanjia.onesmart.org/*"
      ],
      "js": [
        "script/lib/jquery.js",
        "script/contentScript/1v1.js",
        "script/contentScript/1v3.js"
      ]
    }
  ]
}
